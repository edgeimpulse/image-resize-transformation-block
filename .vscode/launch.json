    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--input-project-api-key", "",
                "--output-project-api-key", "",
                "--resize-method", "fit-longest",
                "--resolution", "320",
                "--is-grayscale", "False",
                "--debug","True"
            ]
        }
    ]
}